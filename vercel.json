{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
  ],
  "routes": [
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "^/(.*\\.(js|css|png|jpg|jpeg|svg|json|ico|woff2|woff))$", "dest": "/$1" },
    { "src": "^/service-worker.js$", "dest": "/service-worker.js" },
    { "src": "^(.*)$", "dest": "/index.html" }
  ]
}
